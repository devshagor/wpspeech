{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wpspeech/player",
	"version": "1.0.0",
	"title": "WP Speech Player",
	"category": "widgets",
	"icon": "controls-volumeon",
	"description": "Add a text-to-speech player anywhere in your content. Automatically disables the global auto-insertion for this post.",
	"keywords": [ "tts", "audio", "speech", "listen", "voice", "player" ],
	"supports": {
		"html": false,
		"multiple": false,
		"reusable": false
	},
	"textdomain": "wpspeech",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./editor.css",
	"render": "file:./render.php"
}
